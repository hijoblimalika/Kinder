(function ($, Drupal, drupalSettings) {
    'use strict';

    var Logger = function(isEnabled) {
        var enabled = false;

        this.constructor = function (isEnabled) {
            enabled = !!isEnabled;
        };

        this.log = function(message, data) {
            if (!enabled) {
                return;
            }

            if (data) {
                console.info(message, data);
            }
            else {
                console.log(message);
            }
        };

        this.constructor(isEnabled);
    };
    var logger = new Logger(false);

    var VideoTracker = function (videoObject) {
        var videoStatuses = {
            0: 'Video Start',
            25: 'Video Watched 25%',
            50: 'Video Watched 50%',
            75: 'Video Watched 75%',
            100: 'Video End 100%',
        };

        var video = {
            status: 0,
            url: '',
            title: '',
            flags: {
                videoStarted: false,
            },
        };

        this.constructor = function (videoObject) {
            logger.log('## Video tracking constructor:', videoObject);
            this.object = videoObject;

            if (this.object.length) {
                video.flags.videoStarted = false;

                this.startObservers();
            }
        };

        this.startObservers = function () {
            var self = this;

            // Register push flags.
            var dataLayerPushedFlags = {
                25: false,
                50: false,
                75: false,
                100: false,
            };

            // Observe when play is triggered.
            this.object.on('play', function (e) {
                logger.log('## Video Play:', e.target);
                if (0 === e.target.currentTime) {
                    logger.log('## Video Play Frame - set videoStarted = TRUE; FRAME 0;');

                    video.url = e.target.currentSrc;
                    video.title = e.target.parentNode.dataset.title;
                    video.flags.videoStarted = true;
                }
            });

            this.object.on('timeupdate', function (e) {
                // logger.log('## Video Event:', e);
                // logger.log('## @ Video Duration:', e.target.duration);
                // logger.log('## @ Video Time update:', e.target.currentTime);
                // logger.log('## @ Video Flags:', video.flags);
                // logger.log('## @ Data Layer Push Flags:', dataLayerPushedFlags);

                var currentTime = e.target.currentTime;
                var durationTwentyFive = e.target.duration * 0.25;
                var durationFifty = e.target.duration * 0.5;
                var durationSeventyFive = e.target.duration * 0.75;

                // Set video data;
                video.url = e.target.currentSrc;
                video.title = e.target.parentNode.dataset.title;

                // Video has looped, restart flags.
                if (0 === currentTime && dataLayerPushedFlags["75"]) {
                    logger.log('## Video rewind - autoplay - Reset flags.');
                    dataLayerPushedFlags["25"] = false;
                    dataLayerPushedFlags["50"] = false;
                    dataLayerPushedFlags["75"] = false;

                    // In case before looping we did not registered the time update at the exact duration time.
                    if (!dataLayerPushedFlags["100"]) {
                        logger.log('## Video has not passed 100%, push it.');
                        self.videoEnded(e);
                    }

                    dataLayerPushedFlags["100"] = false;
                }

                // Video has been started, then do a single push to register in GTM.
                if (true === video.flags.videoStarted) {
                    video.status = 0;
                    logger.log('## Video Start:', video);
                    logger.log('### Video Start - Video Started:', video.flags.videoStarted);

                    // Push GTM dataLayer.
                    self.pushDataLayer();

                    // Remove flag in order not to loop in.
                    video.flags.videoStarted = false;
                }

                if (currentTime >= durationTwentyFive && currentTime < durationFifty && !dataLayerPushedFlags["25"]) {
                    video.status = 25;
                    dataLayerPushedFlags["25"] = true;
                    logger.log('## Video Status 25%:', video);
                    logger.log('### Video pushed Flags:', dataLayerPushedFlags);

                    // Push GTM dataLayer.
                    self.pushDataLayer();
                }
                else if (currentTime >= durationFifty && currentTime < durationSeventyFive && !dataLayerPushedFlags["50"]) {
                    video.status = 50;
                    dataLayerPushedFlags["50"] = true;
                    logger.log('## Video Status 50%:', video);
                    logger.log('### Video pushed Flags:', dataLayerPushedFlags);

                    // Push GTM dataLayer.
                    self.pushDataLayer();
                }
                else if (currentTime >= durationSeventyFive && currentTime < 100 && !dataLayerPushedFlags["75"]) {
                    video.status = 75;
                    dataLayerPushedFlags["75"] = true;
                    logger.log('## Video Status 75%:', video);
                    logger.log('### Video pushed Flags:', dataLayerPushedFlags);

                    // Push GTM dataLayer.
                    self.pushDataLayer();
                }
                // In cases where we have an auto loop and ended is not triggered.
                else if(currentTime === e.target.duration && !dataLayerPushedFlags["100"]) {
                    dataLayerPushedFlags["100"] = true;
                    logger.log('## Video Status 100%:', video);
                    logger.log('### Video pushed Flags:', dataLayerPushedFlags);
                    self.videoEnded(e);
                }
            });

            this.object.on("ended", function (e) {
                logger.log('## Video Ended:', e.target);

                // In cases we already triggered that on the timeupdate.
                if (!dataLayerPushedFlags["100"]) {
                    self.videoEnded(e);
                }
            });
        };

        this.videoEnded = function (e) {
            logger.log('## Called video ended.');
            video.status = 100;

            video.url = e.target.currentSrc;
            video.title = e.target.parentNode.dataset.title;

            logger.log('## Video Status 100%:', video);

            // Push GTM dataLayer.
            this.pushDataLayer();
        };

        this.pushDataLayer = function () {
            logger.log('## Video Push Data Layer:', {
                'event': 'video',
                'category': 'Video',
                'attributes.videostatus': videoStatuses[video.status],
                'attributes.videoUrl': video.url,
                'attributes.videoTitle': video.title,
            });

            typeof dataLayer !== 'undefined' && typeof dataLayer === 'object' && dataLayer.push({
                'event': 'video',
                'category': 'Video',
                'attributes.videostatus': videoStatuses[video.status],
                'attributes.videoUrl': video.url,
                'attributes.videoTitle': video.title,
            });
        };

        this.constructor(videoObject);
    };

    Drupal.videoEmbedProvider = function() {

        logger.log('!! Attached Native Video GTM Tracking for Context !!');

        $('.video-embed-field-provider-ferrero-video-platform video:not(".is-gtm-tracked")').each(function () {
            logger.log('Start video tracking for:', $(this));
            $(this).addClass('is-gtm-tracked');

            // @TODO Remove IF after CEO test - Disable video tracking of autoplay videos.
            if ($(this).parents('.video-player').length > 0) {
                new VideoTracker($(this));
            }
        });

        // Observe special case where we clone the media in another container where it is displayed.
        $('.video-collection-thumbnails .video-collection-thumbnail').click(function () {
            $(this).parents('.paragraph-media').find('.video-collection-player .video-embed-field-provider-ferrero-video-platform video:not(".is-gtm-tracked-and-cloned")').each(function () {
                $(this).addClass('is-gtm-tracked-and-cloned');
                new VideoTracker($(this));
            });
        });

        // Track also the first video from collection when play is being pressed.
        $('.paragraph--type--kinder-story-video-collection .video-collection-player-play-button').click(function () {
            var $parent = $(this).parent().find('.video-embed-field-provider-ferrero-video-platform video:not(".is-gtm-tracked-and-cloned")');
            if ($parent.length > 0) {
                $parent.addClass('is-gtm-tracked-and-cloned');
                new VideoTracker($parent);
            }
        });

        var videoContainer = $('.paragraph--type--kinder-story-video-collection:not(".has-registered-observer")');
        if (videoContainer.length > 0) {
            // Create an observer instance.
            var observer = new MutationObserver(function (mutations) {
                mutations.forEach(function (mutation) {
                    logger.log('Registered mutation over kinder-story-video-collection:', mutations);

                    var newNodes = mutation.addedNodes;
                    logger.log('Mutations triggered, new nodes:', newNodes);

                    // If there are new nodes added.
                    if (newNodes !== null) {
                        var $nodes = $(newNodes);

                        $nodes.each(function () {
                            logger.log('New node being created', $(this));
                            new VideoTracker($(this));
                        });
                    }
                });
            });

            // Observe dynamically created video sections not registered at page rendering.
            videoContainer.each(function () {
                $(this).addClass('has-registered-observer');
                logger.log('Found dynamic video container:', videoContainer);
                // Pass in the target node, as well as the observer options.
                observer.observe(this, {
                    attributes: true,
                    childList: true,
                    characterData: true
                });
            });
        }
    };

})(jQuery, Drupal, drupalSettings);
;
(function ($, Drupal, drupalSettings) {
    'use strict';

    var Logger = function(isEnabled) {
        var enabled = false;

        this.constructor = function (isEnabled) {
            enabled = !!isEnabled;
        };

        this.log = function(message, data) {
            if (!enabled) {
                return;
            }

            if (data) {
                console.info(message, data);
            }
            else {
                console.log(message);
            }
        };

        this.constructor(isEnabled);
    };
    var logger = new Logger(false);

    Drupal.behaviors.ctaGtmTracking = {
        attach: function (context, setting) {
            var events = {
                DiscoverNow: {
                    'eventCategory': 'CTA',
                    'event': 'DiscoverNow',
                    'eventAction': 'Click_DiscoverNow',
                    'eventLabel': '{landingpageurl}',
                },
                DiscoverMore: {
                    'eventCategory': 'CTA',
                    'event': 'DiscoverMore',
                    'eventAction': 'Click_DiscoverMore',
                    'eventLabel': '{landingpageurl}',
                },
                KinderStory: {
                    'eventCategory': 'CTA',
                    'event': 'KinderStory',
                    'eventAction': 'Click_KinderStory',
                },
                DiscMoreMain: {
                    'eventCategory': 'CTA',
                    'event': 'DiscMoreMain',
                    'eventAction': 'Click_DiscMoreMain',
                    'eventLabel': '',
                },
            };

            if (typeof dataLayer !== 'undefined') {
                $('.paragraph--type--tile-grid .tile-container.absolute-link .field--type-link a', context).click(function () {
                    var data = {
                        'event': 'DiscoverArticle',
                        'eventCategory': 'CTA',
                        'eventAction': 'Click_DiscoverArticle',
                        'eventLabel': this.href
                    };

                    logger.log('dataLayer.push >> Article:', data);
                    dataLayer.push(data);
                });

                $('.node--type-ingredient a.absolute-link', context).click(function () {
                    var data = {
                        'event': 'ClickIngredient',
                        'eventCategory': 'CTA',
                        'eventAction': 'Click_Ingredient',
                        'eventLabel': $(this).data('ingredient-type')
                    };

                    logger.log('dataLayer.push >> Article:', data);
                    dataLayer.push(data);
                });

                $('.paragraph.paragraph--type--kinder-ingredient-hub-item .absolute-link .field--name-link > a', context).click(function(){
                    var data = {
                      'event': 'ClickIngredient',
                      'eventCategory': 'CTA',
                      'eventAction': 'Click_Ingredient',
                      'eventLabel': this.getAttribute('href'),
                    };
                    logger.log('dataLayer.push >> Ingredient large.', data)
                    dataLayer.push(data);
                });

                $('.gtm-cta-tracking .cta-button a, .gtm-cta-tracking.paragraph--type--cta .field--name-link a', context).click(function () {
                    var data = {};
                    var event = $(this).parents('.gtm-cta-tracking').data('gtm-cta-event').toString();
                    logger.log('CTA Event:', event);

                    if (event.indexOf('DiscMoreMain') !== -1) {
                        data = events.DiscMoreMain;
                        data.eventLabel = event.split('-')[1];

                        logger.log('CTA Event Label:', data.eventLabel);
                    }
                    else {
                        data = events[event];
                    }

                    logger.log('CTA Event Data:', data);

                    // Set current path if required.
                    if (
                        typeof data === 'object'
                        && typeof data.eventLabel !== 'undefined'
                        && '{landingpageurl}' === data.eventLabel
                    ) {
                        data.eventLabel = this.getAttribute('href');
                    }

                    logger.log('dataLayer.push >> CTA EVENT:', data);
                    dataLayer.push(data);
                });
            }
        }
    };
})(jQuery, Drupal, drupalSettings);
;
