(function ($, Drupal, drupalSettings) {
  'use strict';

  Drupal.behaviors.ieSVG = {
    attach: function (context, setting) {
      $('.paragraph--type--kinder-full-width-header').once().each(function () {
        $(this).find('.full-width-headers-carousel-wrapper').children().once().each(function () {
          backgroundImage($(this));
        });
      });

      $('.paragraph--type--full-width-cross-link', context).once().each(function () {
        var svg = '<svg class="media_ie" baseProfile="full" version="1.2"><defs><mask id="crossLinkMask" maskUnits="userSpaceOnUse" maskContentUnits="userSpaceOnUse" transform="scale(1)"><image id="ie_mask" width="99.5vw" height="100.5vh" xlink:href="data:image/png;base64,"></image></mask></defs><filter id="brightness"><feComponentTransfer><feFuncR type="linear" slope="0.85"></feFuncR><feFuncG type="linear" slope="0.85"></feFuncG><feFuncB type="linear" slope="0.85"></feFuncB> </feComponentTransfer></filter><image class="cross-link" filter="url(#brightness)" mask="url(#crossLinkMask)" width="101vw" height="101vh" xlink:href=""></image></svg>';
        var imgSrc = $(this).find('picture img').attr('src');

        applyImage($(this), svg);

        $(this).find('.cross-link').attr('xlink:href', imgSrc);
      });

      $('.hub-ingredient.ingredient-header', context).once().each(function () {
        var svg = '<svg id="ie_or_edge" class="media_ie" baseProfile="full" version="1.2" style="display: none;"> <defs> <mask id="ingrediantMask" maskUnits="userSpaceOnUse" maskContentUnits="userSpaceOnUse" transform="scale(1)"> <image width="33vw" height="29vw" fill="white;" xlink:href="data:image/svg+xml;base64,"></image> </mask> </defs>  <image class="header-ingredient-image" id="ie_image" style="top: 1.5vw;" mask="url(#ingrediantMask)" width="33vw" height="29vw" xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="" /></svg>';
        var imgSrc = $(this).find('.field--name-field-media-image > img').attr('src');

        applyImage($(this), svg);

        $(this).find('.header-ingredient-image').attr('xlink:href', imgSrc);
      });

      $('.field--name-kinder-ingredients > .field__item', context).once().each(function () {
        var svg = '<svg class="ie_or_edge header-ingredient-image" baseProfile="full" version="1.2" width="5.41667vw" height="5.41667vw"><defs><mask id="ingredient-mask" maskUnits="userSpaceOnUse" maskContentUnits="userSpaceOnUse" transform="scale(1)"><image class="trus-header-ingredient-image-mask" width="5.41667vw" height="5.41667vw" fill="white" xlink:href="data:image/png;base64,"></image></mask></defs><image mask="url(#ingredient-mask)" class="header-ingredient-selector-ie-image" width="5.41667vw" height="5.41667vw" y="0" x="0" xlink:href=""></image> </svg>';
        var imgSrc = $(this).find('.field--name-image > img').attr('src');

        var isIE = !!navigator.userAgent.match(/Trident.*rv\:11\./);

        if (isIE) {
          $('body').addClass('ie11');
          $(this).find('.field--name-image').append(svg);
          $(this).find('.image-style-ingredients-selector-image').hide();
          $(this).find('.media_ie').show();
        }

        $(this).find('.header-ingredient-selector-ie-image').attr('xlink:href', imgSrc);
      });

      $('.js-carousel-ingredient-hub-slider > div', context).eq(0).addClass('first');
      $('.js-carousel-ingredient-hub-slider > div', context).once().each(function(index, elem) {
        if ($(this).hasClass('first')) {
          var svg = '<svg class="ie_edge header-ingredient-hub-image" baseProfile="full" version="1.2" width="24.0167vw" height="23.4167vw"><defs><mask id="ingredientHubImage" maskUnits="userSpaceOnUse" maskContentUnits="userSpaceOnUse" transform="scale(1)"><image class="trust-header-ingredient-image-mask" width="24.0167vw" height="23.4167vw" xlink:href="data:image/svg+xml;base64,"></image> </mask></defs><image mask="url(#ingredientHubImage)" class="front-image" y="0" x="0" width="24.4167vw" height="23.4167vw" xlink:href="" ></image><image mask="url(#ingredientHubImage)" class="back-image" width="24.4167vw" height="23.4167vw" y="0" x="0" xlink:href=""></image></svg>';
        }
        else {
          var svg = '<svg class="ie_edge second" baseProfile="full" version="1.2" width="24.0167vw" height="23.4167vw"><defs><mask id="ingredientHubImage2" maskUnits="userSpaceOnUse" maskContentUnits="userSpaceOnUse" transform="scale(1)"><image class="trust-header-ingredient-image-mask" width="24.0167vw" height="23.4167vw"  xlink:href="data:image/svg+xml;base64,"></image> </mask></defs><image mask="url(#ingredientHubImage2)" class="front-image" y="0" x="0" width="24.4167vw" height="23.4167vw" xlink:href=""></image><image mask="url(#ingredientHubImage2)" class="back-image" y="0" x="0" width="24.4167vw" height="23.4167vw" xlink:href=""></image></svg>';
        }

        var imgSrc = $(this).find('.field--name-image .field--name-field-media-image img').attr('data-src');
        var imgSrcHover = $(this).find('.field--name-hover-image .field--name-field-media-image img').attr('data-src');
        var isIE = !!navigator.userAgent.match(/Trident.*rv\:11\./);

        if (imgSrc === undefined && imgSrcHover === undefined) {
          imgSrc = $(this).find('.field--name-image .field--name-field-media-image > img').attr('src');
          imgSrcHover = $(this).find('.field--name-hover-image .field--name-field-media-image > img').attr('src');
        }

        if (isIE) {
            $('body').addClass('ie11');
            $(this).find('.field--name-image').append(svg);
            $(this).find('.field--name-field-media-image').hide();
            $(this).find('.media_ie').show();
            $(this).find('.front-image').attr('xlink:href', imgSrc);
            $(this).find('.back-image').attr('xlink:href', imgSrcHover);
          }
      });

      $('.paragraph--type--kinder-main-carousel-home-slide').children().each(function () {
        backgroundImage($(this));
      });
    }
  };

  var applyImage = function ($this, svg) {
    var isIE = !!navigator.userAgent.match(/Trident.*rv\:11\./);

    if (isIE) {
      $('body').addClass('ie11');
      $this.find('.media--type-image').append(svg);
      $this.find('.field--name-field-media-image').hide();
      $this.find('.media_ie').show();
    }
  };

  var backgroundImage = function ($this) {
    var isIE = !!navigator.userAgent.match(/Trident.*rv\:11\./);

    if (isIE) {
      var image = $this.find('img');

      if (image.length > 0) {
        image.parent().hide();
        image.parent().parent().css({
          'background-image': 'url(' + image.attr('src') + ')',
          'background-position': 'center center',
          'background-origin': 'content-box',
          'background-size': 'cover',
          'height': '100%'
        });
      }
    }
  }
})(jQuery, Drupal, drupalSettings);
;
(function ($, Drupal, drupalSettings) {
  'use strict';

  Drupal.objectMagic = {
    extend: function (objects) {
      var dummy = {};

      if (typeof objects === 'object') {
        objects.forEach(function(object) {
          if (typeof object !== 'object') {
            return;
          }

          for (var property in object) {
            if (object.hasOwnProperty(property)) {
              dummy[property] = object[property];
            }
          }
        });
      }

      return dummy;
    }
  };

  Drupal.behaviors.homeMainCarousel = {
    attach: function (context, setting) {
      var dir = $('html').attr('dir'),
        dirRTL = false;

      if (dir === 'rtl') {
        dirRTL = true;
      }

      $('.paragraph--type--kinder-main-carousel-home .swiper-wrapper').once().each(function () {
        if ($(this).children().length > 1) {
          homeCarouselFunc($(this), dirRTL);
        }
      });

      $('.main-carousel-images').once().on('click', function () {
        $(this).parent().find('.cta-button > a').trigger('click');
      });

    }
  };

  Drupal.swiper = {
    defaultSetup: {
      loop: true,
      speed: 500,
      autoplay: false,
    },
    init: function (target, setup) {
      if ('object' !== typeof target) {
        return {};
      }

      if (typeof setup !== 'object') {
        setup = {};
      }

      var carouselSetup = Drupal.objectMagic.extend([this.defaultSetup, setup]);

      return new Swiper(target, carouselSetup);
    },
    destroy: function (object) {
      if ('object' === typeof object && object instanceof Swiper) {
        object.destroy();

        return true;
      }

      return false;
    }
  };

  var homeCarouselFunc = function ($this, dirRTL) {
    var homeCarousel = null;
    var homeCarouselSetup = {
      loop: true,
      speed: 500,
      effect: 'slide',
      preventClicksPropagation: false,
      navigation: {
        nextEl: !dirRTL ? $this.parent().find('.js-button-next') : $this.parent().find('.js-button-previous'),
        prevEl: !dirRTL ? $this.parent().find('.js-button-previous') : $this.parent().find('.js-button-next'),
      },
      autoplay: {
        delay: 5000,
      },
      pagination: {
        el: $this.parent().find('.carousel-pagination'),
        clickable: true
      },
    };

    var homeCarouselDesktopSetup = {
      effect: 'fade',
      fadeEffect: {
        crossFade: true,
      },
      preventClicks: false,
      allowTouchMove: false,
    };

    if ($(window).width() < 900) {
      Drupal.swiper.destroy(homeCarousel);

      homeCarousel = Drupal.swiper.init(
        $this.parent(),
        homeCarouselSetup
      );

      elementOpacity(parseInt(homeCarousel.realIndex));

      homeCarousel.on('slideChange', function () {
        Drupal.blazy.init.revalidate();
      });
    }
    else {
      Drupal.swiper.destroy(homeCarousel);

      homeCarousel = Drupal.swiper.init(
        $this.parent(),
        Drupal.objectMagic.extend([homeCarouselSetup, homeCarouselDesktopSetup])
      );

      homeCarousel.on('slideChange', function () {
        Drupal.blazy.init.revalidate();
      });
    }

    window.addEventListener('orientationChange', function () {
      elementOpacity(parseInt(homeCarousel.realIndex));

      if ($(window).width() < 900) {
        Drupal.swiper.destroy(homeCarousel);

        homeCarousel = Drupal.swiper.init(
          $this.parent(),
          homeCarouselSetup
        );
      }
      else {
        Drupal.swiper.destroy(homeCarousel);

        homeCarousel = Drupal.swiper.init(
          $this.parent(),
          Drupal.objectMagic.extend([homeCarouselSetup, homeCarouselDesktopSetup])
        );
      }

      homeCarousel.on('slideChange', function () {
        elementOpacity(parseInt(homeCarousel.realIndex));
        Drupal.blazy.init.revalidate();
      });
    });

    homeCarousel.on('slideChange', function () {
      elementOpacity(parseInt(homeCarousel.realIndex));
      Drupal.blazy.init.revalidate();
    });
  };

  var elementOpacity = function (index) {
    var currentSlide = $('.paragraph--type--kinder-main-carousel-home [data-swiper-slide-index="' + index + '"]');

    if ($(window).width() < 900) {
      if (!currentSlide.hasClass('full-width-image')) {
        currentSlide.parent().parent().find('.carousel-pagination').css({'opacity': '1', 'z-index': '0'});
        currentSlide.parent().parent().find('.carousel-pagination').css({'top': currentSlide.find('.main-carousel-images').height() })
      }
      else {
        currentSlide.parent().parent().find('.carousel-pagination').css({'opacity': '0', 'z-index': '-1'});
      }
    }

    $('.main-carousel-images').once().on('click', function (e) {
      e.preventDefault();

      var element = $(this).parent().find('.cta-button > a');
      var target = element.attr('target') ? '_blank' : '_self';

      window.open(element.attr("href"), target);
    });
  };
})(jQuery, Drupal, drupalSettings);
;
(function ($, Drupal, drupalSettings) {
    'use strict';

    var Logger = function(isEnabled) {
        var enabled = false;

        this.constructor = function (isEnabled) {
            enabled = !!isEnabled;
        };

        this.log = function(message, data) {
            if (!enabled) {
                return;
            }

            if (data) {
                console.info(message, data);
            }
            else {
                console.log(message);
            }
        };

        this.constructor(isEnabled);
    };
    var logger = new Logger(false);

    Drupal.behaviors.recipeGtmTracking = {
        events: {
            socialSharing: {
                'event': 'social-share',
                'eventActionsocial': '{social-type}',
                'eventLabelsocial': '{landingpageurl}',
            },
            download: {
                'downloadtype': 'document',
                'event': 'download',
                'downloadname': '',
            },
            prevNext: {
                'eventCategoryprevnext': '{button-type}',
                'event': 'prev-next',
                'eventActionprevnext': '{landingpageurl}',
                'eventLabelprevnext': '{click-page-url}',
            }
        },
        attach: function (context, setting) {
            if (typeof dataLayer !== 'undefined') {
                var object = this;
                jQuery('[class^="a2a_button"]', context).click(function () {
                    var data = object.events.socialSharing;

                    data.eventActionsocial = this.className.replace('a2a_button_', '');
                    data.eventLabelsocial = window.location.href;

                    logger.log('dataLayer.push >> Social share:', data);
                    dataLayer.push(data);
                });

                jQuery('a.file-url', context).click(function () {
                    var data = object.events.download;
                    data.downloadname = this.getAttribute('href');

                    logger.log('dataLayer.push >> Download:', data);
                    dataLayer.push(data);
                });

                jQuery('a.file-url-download', context).click(function () {
                    var data = object.events.download;
                    data.downloadname = this.getAttribute('href');
                    data.event = 'Download.PDF';
                    logger.log('dataLayer.push >> Download:', data);
                    dataLayer.push(data);
                });

                jQuery('.paragraph--type--kinder-previous-and-next-nodes a', context).click(function () {
                    var data = object.events.prevNext;
                    data.eventActionprevnext = window.location.href;
                    data.eventLabelprevnext = this.getAttribute('href');

                    data.eventCategoryprevnext = 'back';
                    if (jQuery(this).parent().hasClass('field--name-kinder-previous-node')) {
                        data.eventCategoryprevnext = 'previous';
                    } else if (jQuery(this).parent().hasClass('field--name-kinder-next-node')) {
                        data.eventCategoryprevnext = 'next';
                    }

                    logger.log('dataLayer.push >> Previous Next:', data);
                    dataLayer.push(data);
                });
            }
        },
    };
})(jQuery, Drupal, drupalSettings);
;
(function ($, Drupal, drupalSettings) {
  'use strict';

  var Logger = function(isEnabled) {
    var enabled = false;

    this.constructor = function (isEnabled) {
      enabled = !!isEnabled;
    };

    this.log = function(message, data) {
      if (!enabled) {
        return;
      }

      if (data) {
        console.info(message, data);
      }
      else {
        console.log(message);
      }
    };

    this.constructor(isEnabled);
  };
  var logger = new Logger(false);
  var v = 'eventN'+Math.random();
  Drupal.behaviors.recipeGtmTracking = {
    // events: {
    //   imageParagraph: {
    //     'event': 'image.LinkClicks',
    //     'attribute.ImageClicks': '{url}',
    //   },
    //   productImage: {
    //     'event': 'product.LinkClicks',
    //     'productClickLabel': '{product_name}',
    //   },
    //   mainCarousel: {
    //     'event': 'main.CarouselLinkClicks',
    //     'attribute.BannerClicks': '{banner_clicks}',
    //     'attribute.mainButtonClicks':'{button_name}',
    //     'attribute.mainUrl':'{banner_url, button_clicks}',
    //   },
    //   socialMedia: {
    //     'event':'social.MediaClicks',
    //     'attribute.SocialMedia':'{social_network_name}', // Twitter, Facebook, Instagram etc . . .
    //   },
    //   relatedContent: {
    //     'event':'related.Content',
    //     'attribute.relatedTitle': '{heading}',
    //     'attribute.relatedUrl': '{embed_url}',
    //   },
    //   paragraphTabs: {
    //     'event':'tabs.Clicks',
    //     'attribute.ButtonName':'{button_name}',
    //     'attribute.TabsUrl': '{tab_url}',
    //   }
    //
    // },
    attach: function (context, setting) {
      if (typeof dataLayer !== 'undefined') {
        var object = this;
        // Image paragraph LinkClicks
        jQuery('.paragraph.paragraph--type--kinder-images-slide .container .absolute-link .field--name-link > a', context).click(function () {

          var data = {
            'event': 'image.LinkClicks',
            'attribute.ImageClicks': this.getAttribute('href'),
          };
          logger.log('  dataLayer.push >> Image Paragraph Link Clicks:', data);
          dataLayer.push(data);
        });

        // Product LinkClicks
        jQuery('.field--name-kinder-hub-item .field__item .with-hover-image .absolute-link.hover-image .field--name-link > a',context).click(function (){
          var data = {
            'event': 'product.LinkClicks',
            'attribute.productClicks':this.href.substring(this.href.lastIndexOf('/') + 1)
          };
          logger.log('  dataLayer.push >> Product Link Clicks:');
          dataLayer.push(data);

        });

        // Main carousel LinkClicks.
        jQuery('.product-details-slide.swiper-slide .product-details-slide--inner .left-content .field-group-wrapper .cta-button > a',context).click(function (){

          if (jQuery(this).parents('.full-width-image').length) {
            var data = {
              'event': 'mainCarousel',
              'attribute.BannerClicks': 'bannerClick',
              'attribute.mainButtonClicks': '',
              'attribute.mainUrl': this.getAttribute('href'),
            };
          } else {
            var data = {
              'event': 'mainCarousel',
              'attribute.BannerClicks': '',
              'attribute.mainButtonClicks': jQuery(this).find('.button-cta-label .label-item').text().trim(),
              'attribute.mainUrl': this.getAttribute('href'),
            };
          }

          logger.log('  dataLayer.push >> Main Carousel Link Clicks:', data);
          dataLayer.push(data);
        });

        // Social MediaClicks
        jQuery('.paragraph.paragraph--type--kinder-social-content-slide ',context).click(function () {
          var data = {
            'event':'social.MediaClicks',
            'attribute.SocialMedia': new URL(jQuery(this).find('.absolute-link .field--name-kinder-social-media-link .link-item .link-url > a')[0].getAttribute('href')).host
          };
          logger.log('  dataLayer.push >> Social Media Link Clicks:', data);
          dataLayer.push(data);
        });

        // Related Content
        jQuery('.paragraph.paragraph--type--kinder-related-content .related-content-wrapper .result-cell .result-cell-inner article.node--type-landing-page ', context).click(function(){
          var data = {
            'event':'relatedContent',
            'attribute.relatedTitle': jQuery(this).find('h3').text().trim(),
            'attribute.relatedUrl': jQuery(this).find('a')[0].getAttribute('href')
          };
          logger.log('  dataLayer.push >> Product Link Clicks:', data);
          dataLayer.push(data);
        });

        //Paragraph Tabs
        jQuery('.paragraph.paragraph--type--product-tabs .field--name-tabs .field__item .paragraph--type--product-tabs-item .field--name-link > a', context).click(function(){
          var data = {
            'event': 'paragraphTabs',
            'attribute.buttonName': jQuery(this).text().trim(),
            'attribute.TabsUrl': this.getAttribute('href'),
          };
          logger.log('  dataLayer.push >> Paragraph Tabs Clicks:', data);
          dataLayer.push(data);
        });

      }
    },
  };

  Drupal.behaviors.videoFilterAndPlayButtonTracking = {
    attach: function (context, setting) {
      // Video List Paragraph tracking - age, range, topology.
