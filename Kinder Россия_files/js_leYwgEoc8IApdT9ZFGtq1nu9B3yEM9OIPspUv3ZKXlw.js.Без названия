(function ($, Drupal, drupalSettings) {
  'use strict';

  Drupal.behaviors.posterYoutube = {
    attach: function (context, setting) {
      if($(window).width() < 1025) {
        $('.paragraph--type--video [data-poster]', context).once().each(function () {
          var $poster = $(this).find('.hidden-poster');
          if ($poster.length > 0) {
            $(this).find('iframe').hide();

            $poster.attr('alt', Drupal.t('Poster gif image'));
            $poster.show();

            $(this).parent().parent().parent().addClass('with-poster');
          }
        });
      }
    }
  };
})(jQuery, Drupal, drupalSettings);
;
(function ($, Drupal, drupalSettings) {
  'use strict';

  Drupal.objectMagic = {
    extend: function (objects) {
      var dummy = {};

      if (typeof objects === 'object') {
        objects.forEach(function(object) {
          if (typeof object !== 'object') {
            return;
          }

          for (var property in object) {
            if (object.hasOwnProperty(property)) {
              dummy[property] = object[property];
            }
          }
        });
      }

      return dummy;
    }
  };

  Drupal.swiper = {
    defaultSetup: {
      loop: true,
      speed: 500,
      autoplay: false,
    },
    init: function (target, setup) {
      if ('object' !== typeof target) {
        return {};
      }

      if (typeof setup !== 'object') {
        setup = {};
      }

      var carouselSetup = Drupal.objectMagic.extend([this.defaultSetup, setup]);

      return new Swiper(target, carouselSetup);
    },
    destroy: function (object) {
      if ('object' === typeof object && object instanceof Swiper) {
        object.destroy();

        return true;
      }

      return false;
    }
  };

  Drupal.behaviors.kinderTiles = {
    attach: function (context, setting) {
      $('.boxes .swiper-wrapper').once().each(function () {
        if ($(this).children().length > 1) {
          tileGridBoxesFunc($(this));
        }
      });
    }
  };

  var tileGridBoxesFunc = function ($this) {
    var tileGridBoxesSetup = {
      direction: 'horizontal',
      pagination: {
        el: $this.next('.carousel-pagination'),
        clickable: true
      },
    };

    if ($(window).width() < 900) {
      var tileGridBoxes = Drupal.swiper.init(
        $this.parent(),
        tileGridBoxesSetup
      );
    }

    $(window).on('resize', function () {
      if ($(window).width() >= 900 && typeof tileGridBoxes !== 'undefined') {
        tileGridBoxes.destroy();
        tileGridBoxes = undefined;
      }

      if ($(window).width() < 900 && typeof tileGridBoxes === 'undefined') {
        tileGridBoxes = Drupal.swiper.init(
            $this.parent(),
            tileGridBoxesSetup
        );
      }
    });
  };
})(jQuery, Drupal, drupalSettings);
;
